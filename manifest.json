{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "0.0.1",
  "description": "The \"Time Tracker\" Chrome Extension made by me Achyut Singh is for the eLitmus test.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/js/content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "assets/img/logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup.html", "statistics.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "script-src": "'self' https://cdnjs.cloudflare.com"
  }
}
